            "Appointment": {
                "required": [
                    "patient_id",
                    "practitioner_id",
                    "service_id",
                    "appointment_date",
                    "appointment_time"
                ],
                "properties": {
                    "patient_id": {
                        "type": "integer"
                    },
                    "practitioner_id": {
                        "type": "integer"
                    },
                    "service_id": {
                        "type": "integer"
                    },
                    "appointment_date": {
                        "type": "string",
                        "format": "date"
                    },
                    "appointment_time": {
                        "type": "string",
                        "format": "time"
                    },
                    "status": {
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "Appointments",
            "description": "API Endpoints for managing appointments"
        },
        {
            "name": "Swagger Test",
